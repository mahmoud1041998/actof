<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.image.*?>
<?import org.controlsfx.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane fx:id="content" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <HBox fx:id="titleBox" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane fx:id="titleBox" HBox.hgrow="NEVER" />
            <HBox fx:id="titleContentBox" />
            <Separator fx:id="separator" visible="false" HBox.hgrow="ALWAYS" />
            <AnchorPane fx:id="descriptionBox" />
         </children>
      </HBox>
   </top>
   <center>
      <HBox fx:id="contentBox" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
