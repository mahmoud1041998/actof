<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!-- public IGenerator(String aRootPath, String aSourcePath, String aOutputPath, IMetamodel aMetamodel, ITemplateContext 
		aContext, Facet<Kind<?>> aFacet) -->
	<bean id="generator" class="net.objectof.actof.schemaspy.generator.IGenerator">
		<!-- root -->
		<constructor-arg value="." />
		<!-- source path within root -->
		<constructor-arg value="src/test/resources/models" />
		<!-- output path within root -->
		<constructor-arg value="src/test/java" />
		<!-- meta model -->
		<constructor-arg>
			<bean class="net.objectof.model.impl.IBaseMetamodel" />
		</constructor-arg>
		<!-- template context -->
		<constructor-arg ref="context" />
		<!-- facet to process -->
		<constructor-arg>
			<bean class="net.objectof.facet.impl.IFacets">
				<constructor-arg value="test.objectof.org:1401/mm/generate" />
				<constructor-arg>
					<list>
						<bean class="net.objectof.actof.schemaspy.generator.templates.IInterfaceTemplate">
							<constructor-arg ref="context" />
						</bean>
						<bean class="net.objectof.actof.schemaspy.generator.templates.ICompositeTemplate">
							<constructor-arg ref="context" />
						</bean>
						<bean class="net.objectof.actof.schemaspy.generator.templates.IPropertiesTemplate">
							<constructor-arg ref="context" />
						</bean>
					</list>
				</constructor-arg>
			</bean>
		</constructor-arg>
	</bean>
	<bean id="context" class="net.objectof.actof.schemaspy.generator.velocity.ITemplateContext">
		<constructor-arg value="" />
	</bean>
</beans>